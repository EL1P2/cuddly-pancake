// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Employee {
    id          Int             @id         @default(autoincrement())
    name        String
    position    String
    office      String
    age         Int
    startDate   DateTime
    salary      Int
}
